<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ingzone.dao.ResumeDao">

    <insert id="uploadResume" parameterType="com.ingzone.model.dto.Resume">
        INSERT INTO resume (`name`,`date`,`group`,introduce,mail,phone,qq)
        VALUES (#{name},now(),#{group},#{introduce},#{mail},#{phone},#{qq})
    </insert>
</mapper>